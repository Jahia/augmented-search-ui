{
  "name": "augmented-search-ui",
  "version": "3.1.0-SNAPSHOT",
  "description": "Jahia module that provides integration with ES Search UI on top of Jahia Augmented Search API",
  "main": "main.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && yarn lint:fix && webpack",
    "build:nolint": "export NODE_OPTIONS=--openssl-legacy-provider && webpack",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn build -p",
    "build:production-analyze": "yarn build -p --analyze",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && yarn webpack --watch",
    "clean": "rimraf *.log src/main/resources/javascript/app",
    "clean:all": "yarn clean && rimraf node_modules node",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "./scripts/release.sh",
    "sync-pom": "sync-pom-version --use-yarn"
  },
  "author": "Jahia Team",
  "repository": "git+https://github.com/Jahia/search-ui-academy.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^8.0.0-beta.2",
    "@babel/core": "^8.0.0-beta.2",
    "@babel/eslint-parser": "^8.0.0-beta.2",
    "@babel/plugin-external-helpers": "^8.0.0-beta.2",
    "@babel/plugin-transform-class-properties": "^8.0.0-beta.2",
    "@babel/plugin-transform-runtime": "^8.0.0-beta.2",
    "@babel/preset-env": "^8.0.0-beta.2",
    "@babel/preset-react": "^8.0.0-beta.2",
    "@cyclonedx/webpack-plugin": "^2.0.2",
    "auto": "^11.1.2",
    "babel-loader": "^10.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "cryptiles": "^4.1.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.38.0",
    "eslint-config-xo": "^0.49.0",
    "eslint-config-xo-react": "^0.28.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.7",
    "loader-utils": "^3.3.1",
    "pom-parser": "^1.2.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "sync-pom-version-to-package": "^1.6.1",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@date-io/moment": "^3.2.0",
    "@elastic/react-search-ui": "^1.24.2",
    "@elastic/react-search-ui-views": "^1.24.2",
    "@elastic/search-ui": "^1.24.2",
    "@jahia/i18next": "^1.1.0",
    "@jahia/search-ui-jahia-connector": "^2.0.16",
    "@jahia/test-framework": "^1.3.1",
    "classnames": "^2.5.1",
    "core-js": "^3.46.0",
    "cryptiles": "^4.1.3",
    "es6-symbol": "^3.1.4",
    "fuse.js": "^7.1.0",
    "hoek": "^6.1.3",
    "i18next": "^25.6.0",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.1.0",
    "react-icons": "^5.5.0",
    "react-loadable": "^5.5.0",
    "react-props": "^0.0.3",
    "react-select": "^5.10.2",
    "recompose": "^0.30.0",
    "styled-components": "^6.1.8",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "styled-components": "^6"
}
