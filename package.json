{
  "name": "augmented-search-ui",
  "version": "3.0.0",
  "description": "Jahia module that provides integration with ES Search UI on top of Jahia Augmented Search API",
  "main": "main.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "build": "yarn lint:fix && yarn webpack --mode=development",
    "build:nolint": "webpack",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn lint:fix && yarn webpack --mode=production",
    "build:production-analyze": "yarn build -p --analyze",
    "dev": "yarn webpack --watch",
    "clean": "rimraf *.log src/main/resources/javascript/app",
    "clean:all": "yarn clean && rimraf node_modules node",
    "test": "jest",
    "lint": "yarn eslint --ext js,jsx .",
    "lint:fix": "yarn eslint --ext js,jsx --fix .",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "./scripts/release.sh"
  },
  "author": "Jahia Team",
  "repository": "git+https://github.com/Jahia/search-ui-academy.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@jahia/eslint-config": "^1.1.0",
    "auto": "^7.12.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^1.0.0",
    "cryptiles": ">=4.1.2",
    "css-loader": "^1.0.1",
    "eslint": "^7.12.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^2.0.0",
    "hoek": ">=4.2.1",
    "husky": "^4.3.0",
    "pom-parser": "^1.1.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^5.3.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@date-io/moment": "^1.3.5",
    "@elastic/react-search-ui": "1.4.1",
    "@elastic/react-search-ui-views": "1.4.1",
    "@elastic/search-ui": "1.4.1",
    "@jahia/i18next": "^1.0.0-beta.20180628145923",
    "@jahia/search-ui-jahia-connector": "file:.yalc/@jahia/search-ui-jahia-connector",
    "@jahia/test-framework": "^1.0.0-beta.20190329150204",
    "classnames": "^2.2.6",
    "cryptiles": ">=4.1.2",
    "es6-symbol": "^3.1.1",
    "fuse.js": "^3.4.4",
    "hoek": ">=4.2.1",
    "i18next": "^15.0.2",
    "i18next-xhr-backend": "^2.0.0",
    "lodash": "4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.4.0",
    "react-i18next": "^7.3.1",
    "react-icons": "^3.11.0",
    "react-is": "^17.0.1",
    "react-loadable": "^5.5.0",
    "react-props": "^0.0.3",
    "recompose": "^0.30.0",
    "styled-components": "^5.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
